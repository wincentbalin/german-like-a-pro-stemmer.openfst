#
# German Pro Stemmer
#
# From the article https://developer.s24.com/blog/german_stemming_like_a_pro.html

import 'wortliste.grm' as wl;
import 'synonymliste.grm' as sl;

# WORTLISTE is not a self-sufficient wordlist! It should be used with together with hyphenator!
export STEMMER = Optimize[(wl.WORTLISTE <-1>) | (sl.SYNONYMLISTE <-4>)];

